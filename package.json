{
  "name": "nlb-scss",
  "version": "1.3.0",
  "description": "NLB SCSS",
  "main": "index.js",
  "scripts": {
    "build:css": "vite build --mode development",
    "build:css:min": "cross-env MINIFY=true CSS_SUFFIX=.min NO_CLEAN=1 vite build --mode production",
    "build": "pnpm run build:css && pnpm run build:css:min",
    "dev": "vite build --watch",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nlbdev/nlb-scss.git"
  },
  "author": "nlbdev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nlbdev/nlb-scss/issues"
  },
  "homepage": "https://github.com/nlbdev/nlb-scss#readme",
  "dependencies": {
    "bootstrap": "^5.3.2",
    "jquery": "^3.7.1",
    "normalize.css": "^8.0.1",
    "popper.js": "^1.16.1",
    "postcss-import": "^16.0.0",
    "postcss-scss": "^4.0.9",
    "postcss-url": "^10.1.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.3",
    "postcss": "^8.4.32",
    "sass": "^1.69.5",
    "vite": "^7.0.2"
  },
  "packageManager": "pnpm@10.12.4"
}
